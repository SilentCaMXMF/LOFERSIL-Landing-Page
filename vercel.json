{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "lofersil.vercel.app"
        }
      ],
      "destination": "https://lofersil.pt/$1",
      "permanent": true
    }
  ]
}
