{
  "mcp": {
    "context7": {
      "name": "context7",
      "type": "remote",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "Authorization": "Bearer ${CONTEXT7_API_KEY}",
        "Content-Type": "application/json"
      },
      "enabled": true,
      "timeout": 30000,
      "retry": {
        "maxAttempts": 5,
        "interval": 5000
      }
    },
    "gemini": {
      "name": "gemini",
      "type": "remote",
      "url": "https://generativelanguage.googleapis.com/v1beta/models/gemini-2.5-flash-image-preview:generateContent",
      "headers": {
        "GEMINI_API_KEY": "${GEMINI_API_KEY}",
        "Content-Type": "application/json"
      },
      "enabled": false,
      "timeout": 60000,
      "retry": {
        "maxAttempts": 3,
        "interval": 2000
      }
    },
    "local-dev": {
      "name": "local-dev",
      "type": "remote",
      "url": "ws://localhost:3000",
      "headers": {
        "Authorization": "Bearer ${LOCAL_DEV_KEY}"
      },
      "enabled": false,
      "timeout": 10000,
      "retry": {
        "maxAttempts": 3,
        "interval": 2000
      }
    },
    "cloudflare": {
      "name": "cloudflare",
      "type": "remote",
      "url": "https://api.cloudflare.com/client/v4/accounts/${CLOUDFLARE_ACCOUNT_ID}/ai/run",
      "headers": {
        "Authorization": "Bearer ${CLOUDFLARE_API_TOKEN}",
        "Content-Type": "application/json"
      },
      "enabled": true,
      "timeout": 30000,
      "retry": {
        "maxAttempts": 5,
        "interval": 2000
      },
      "accountId": "${CLOUDFLARE_ACCOUNT_ID}",
      "models": {
        "text": "@cf/meta/llama-3.1-8b-instruct",
        "image": "@cf/blackforestlabs/flux-1-schnell",
        "embedding": "@cf/baai/bge-large-en-v1.5"
      }
    }
  }
}
